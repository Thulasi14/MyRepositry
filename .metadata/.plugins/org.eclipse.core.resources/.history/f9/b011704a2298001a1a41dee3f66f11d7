package failureCaseWorking;

import org.testng.IRetryAnalyzer;
import org.testng.ITestResult;

public class AnalyzerClass implements IRetryAnalyzer {
	
	public boolean retry(ITestResult result){
		
		int min=1;
		int maxRun=2;
		
		while(min<=maxRun) {
		
		min++;
		
		return true;
		
		}
		
		return false;
		
	}

}
